<h1><%="#{@user.first_name} #{@user.last_name}"%></h1>
    <div class="row">
      <aside class="col-md-4">
        <section class="user_info">
                <p><%= mail_to "#{@user.email}" %><br>
                <%= "#{@user.address}"%><br>
                <%= "#{@user.city}, #{@user.state}"%><br>
                <%= "#{@user.zip}"%></p>
        </section>
      </aside>

      <div class="col-md-7">
            <% @user_products.each do |product| %>
            <ul>
                <li><%= link_to product.product_name, product%></li>
            </ul>
            <% end %>
      </div>
    </div>
    
   

    Curent Time: <%=Time.now%><br>
    Created At: <%= @user.created_at %><br>
    Updated At: <%= @user.updated_at %><br>